
.ORG 0
	LDI R16,HIGH(RAMEND)
	OUT SPH,R16
	LDI R16,LOW(RAMEND)
	OUT SPL,R16

BACK:
	LDI R16,0x55	
	OUT PORTB,R16	
	RCALL DELAY		
	LDI R16,0xAA	
	OUT PORTB,R16	
	RCALL DELAY		
	RJMP BACK		


DELAY:	LDI		R16,200	
AGAIN:	LDI		R17,250	
HERE:	NOP			
		NOP				
		DEC		R17		
		BRNE	HERE		
		DEC		R16		
		BRNE	AGAIN
		RET
